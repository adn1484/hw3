<h2><%= @places["name"] %> </h2>

<ul>
<% for post in @posts %>
<li>
  <%= post["title"] %>
  <%= post["created_at"] %>
  <%= post["description"] %>
</li>
<% end %>
</ul>

<p> <a href="/posts/new?place_id=<%= @places["id"] %>">Create new post</a> </p>

<p> <a href="/places">Back to all places</a> </p>